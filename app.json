{
  "expo": {
    "name": "Pathly",
    "slug": "pathly",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mresath.pathly",
      "userInterfaceStyle": "automatic"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.mresath.pathly",
      "userInterfaceStyle": "automatic"
    },
    "web": {
      "favicon": "./assets/logo.png",
      "bundler": "metro",
      "userInterfaceStyle": "automatic"
    },
    "extra": {
      "eas": {
        "projectId": "78237ace-22c3-425f-82bd-863f68b82451"
      }
    },
    "owner": "mresath",
    "experiments": {
      "reactCompiler": true
    },
    "plugins": [
      "expo-router",
      [
        "expo-secure-store",
        {
          "configureAndroidBackup": true
        }
      ],
      "expo-localization",
      [
        "expo-image-picker",
        {
          "photosPermission": "This app uses the photo library to allow you to select a profile picture."
        }
      ],
      "react-native-compressor",
      [
        "expo-splash-screen",
        {
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "image": "./assets/logolight.png",
          "dark": {
            "backgroundColor": "#0b1713",
            "image": "./assets/logo.png"
          }
        }
      ]
    ],
    "scheme": "pathly"
  }
}