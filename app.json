{
  "expo": {
    "name": "pathly",
    "slug": "pathly",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "image": "./assets/logo.png"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mresath.pathly",
      "userInterfaceStyle": "automatic"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.mresath.pathly",
      "userInterfaceStyle": "automatic"
    },
    "web": {
      "favicon": "./assets/logo.png",
      "bundler": "metro",
      "userInterfaceStyle": "automatic"
    },
    "extra": {
      "eas": {
        "projectId": "78237ace-22c3-425f-82bd-863f68b82451"
      }
    },
    "owner": "mresath",
    "experiments": {
      "reactCompiler": true
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-localization"
    ],
    "scheme": "pathly"
  }
}
